{
  "name": "frost-builder",
  "version": "0.8.3",
  "author": {
    "name": "Forrest Collins",
    "email": "Forrdev@gmail.com",
    "url": "Forrdev.com"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build:es": "node build.js --es",
    "build:cjs": "node build.js --cjs",
    "build": "npm run build:es && npm run build:cjs",
    "test": "NODE_ENV=test nyc ava --verbose",
    "view-coverage": "npm run test && nyc report --reporter=html",
    "coverage": "npm run test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": ["babel-register"],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
        "src/core/schema.js",
        "tests/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bashkir15/frost/tree/master/packages/frost-builder"
  },
  "license": "MIT",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "bummmble-hive": "^0.1.3",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.0",
    "cosmiconfig": "^3.1.0",
    "css-loader": "^0.28.7",
    "elm-webpack-loader": "^4.4.0",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "frost-utils": "^0.3.1",
    "fs-extra": "^4.0.2",
    "get-port": "^3.2.0",
    "graphql-tag": "^2.5.0",
    "happypack": "^4.0.0",
    "http": "0.0.0",
    "https": "^1.0.0",
    "image-webpack-loader": "^3.4.2",
    "jsome": "^2.3.26",
    "less-loader": "^4.0.5",
    "loader-utils": "^1.1.0",
    "opn": "^5.1.0",
    "ora": "^1.3.0",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.8",
    "resolve-pkg": "^1.0.0",
    "rust-wasm-loader": "^0.1.2",
    "sass-loader": "^6.0.6",
    "serviceworker-webpack-plugin": "^0.2.3",
    "source-map-loader": "^0.2.3",
    "stats-webpack-plugin": "^0.6.1",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-common-shake": "^1.5.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-hot-server-middleware": "^0.2.0",
    "webpack-md5-hash": "0.0.6",
    "webpack-subresource-integrity": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "lint-staged": "^6.0.0",
    "minimist": "^1.2.0",
    "nyc": "^11.3.0",
    "react": "^16.2.0",
    "remap-istanbul": "^0.9.5",
    "rollup": "^0.51.5",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^8.2.6"
  }
}
